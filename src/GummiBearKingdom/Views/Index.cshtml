@model IEnumerable<GummiBearKingdom.Models.Product>

<html>
<head>
    <title>Gummi Bear Kingdom</title>
</head>
<body>
    <h2>World's Best Gummies Bear</h2>
    <h4>@Html.DisplayNameFor(model => model.Name)</h4>
    <ul>
        @foreach (var listProduct in Model)
        {
            <li>@Html.DisplayFor(modelProduct => listProduct.Name)</li>
            <li>@Html.DisplayFor(modelProduct => listProduct.Cost)</li>
            <li>@Html.DisplayFor(modelProduct => listProduct.Country_origin)</li>


        }
    </ul>
</body>
</html>